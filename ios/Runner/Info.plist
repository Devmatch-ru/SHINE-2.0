    <string>Приложению требуется доступ к локальной сети для поиска устройств и трансляции</string>
    <key>NSBonjourServices</key>
	<array>
	<string>_shine._tcp</string>
	<string>_shine._udp</string>
	</array>
    <key>NSAppTransportSecurity</key>
    <dict>
        <key>NSAllowsLocalNetworking</key>
        <true/>
        <key>NSAllowsArbitraryLoads</key>
        <true/>
    </dict>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>Требуется для обнаружения Wi-Fi сетей в локальной сети</string>
